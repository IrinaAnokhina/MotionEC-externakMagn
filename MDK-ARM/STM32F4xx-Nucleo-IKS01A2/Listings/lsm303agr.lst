N/**
N * @brief  Register Component Bus IO operations
N * @param  pObj the device pObj
N * @retval 0 in case of success, an error code otherwise
N */
Nint32_t LSM303AGR_ACC_RegisterBusIO(LSM303AGR_ACC_Object_t *pObj, LSM303AGR_IO_t *pIO)
N{
N  int32_t ret = LSM303AGR_OK;
X  int32_t ret = 0;
N
N  if (pObj == NULL)
X  if (pObj == 0)
N  {
N    ret = LSM303AGR_ERROR;
X    ret = -1;
N  }
N  else
N  {
N    pObj->IO.Init      = pIO->Init;
N    pObj->IO.DeInit    = pIO->DeInit;
N    pObj->IO.BusType   = pIO->BusType;
N    pObj->IO.Address   = pIO->Address;
N    pObj->IO.WriteReg  = pIO->WriteReg;
N    pObj->IO.ReadReg   = pIO->ReadReg;
N    pObj->IO.GetTick   = pIO->GetTick;
N
N    pObj->Ctx.read_reg  = ReadAccRegWrap;
N    pObj->Ctx.write_reg = WriteAccRegWrap;
N    pObj->Ctx.handle    = pObj;
N
N    if (pObj->IO.Init == NULL)
X    if (pObj->IO.Init == 0)
N    {
N      ret = LSM303AGR_ERROR;
X      ret = -1;
N    }
N    else if (pObj->IO.Init() != LSM303AGR_OK)
X    else if (pObj->IO.Init() != 0)
N    {
N      ret = LSM303AGR_ERROR;
X      ret = -1;
N    }
N    else
N    {
N      if (pObj->IO.BusType == LSM303AGR_SPI_3WIRES_BUS) /* SPI 3-Wires */
X      if (pObj->IO.BusType == 1U)  
N      {
N        /* Enable the SPI 3-Wires support only the first time */
N        if (pObj->is_initialized == 0U)
N        {
N          /* Enable SPI 3-Wires on the component */
N          uint8_t data = 0x01;
N
N          if (LSM303AGR_ACC_Write_Reg(pObj, LSM303AGR_CTRL_REG4_A, data) != LSM303AGR_OK)
X          if (LSM303AGR_ACC_Write_Reg(pObj, 0x23U, data) != 0)
N          {
N            return LSM303AGR_ERROR;
X            return -1;
N          }
N        }
N      }
N    }
N  }
N
N  return ret;
N}
