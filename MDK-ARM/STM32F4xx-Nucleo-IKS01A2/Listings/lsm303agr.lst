N/**
N * @brief  Initialize the LSM303AGR sensor
N * @param  pObj the device pObj
N * @retval 0 in case of success, an error code otherwise
N */
Nint32_t LSM303AGR_ACC_Init(LSM303AGR_ACC_Object_t *pObj)
N{
N  /* Enable BDU */
N  if (lsm303agr_xl_block_data_update_set(&(pObj->Ctx), PROPERTY_ENABLE) != LSM303AGR_OK)
X  if (lsm303agr_xl_block_data_update_set(&(pObj->Ctx), (1U)) != 0)
N  {
N    return LSM303AGR_ERROR;
X    return -1;
N  }
N
N  /* FIFO mode selection */
N  if (lsm303agr_xl_fifo_mode_set(&(pObj->Ctx), LSM303AGR_BYPASS_MODE) != LSM303AGR_OK)
X  if (lsm303agr_xl_fifo_mode_set(&(pObj->Ctx), LSM303AGR_BYPASS_MODE) != 0)
N  {
N    return LSM303AGR_ERROR;
X    return -1;
N  }
N
N  /* Select default output data rate. */
N  pObj->acc_odr = LSM303AGR_XL_ODR_100Hz;
N
N  /* Output data rate selection - power down. */
N  if (lsm303agr_xl_data_rate_set(&(pObj->Ctx), LSM303AGR_XL_POWER_DOWN) != LSM303AGR_OK)
X  if (lsm303agr_xl_data_rate_set(&(pObj->Ctx), LSM303AGR_XL_POWER_DOWN) != 0)
N  {
N    return LSM303AGR_ERROR;
X    return -1;
N  }
