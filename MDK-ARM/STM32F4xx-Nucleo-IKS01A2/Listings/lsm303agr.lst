N                  : (Odr <=  100.0f) ? LSM303AGR_XL_ODR_100Hz
N                  : (Odr <=  200.0f) ? LSM303AGR_XL_ODR_200Hz
N                  :                    LSM303AGR_XL_ODR_400Hz;
N
N  return LSM303AGR_OK;
X  return 0;
N}
