N/**
N * @brief  Wrap Read register component function to Bus IO function
N * @param  Handle the device handler
N * @param  Reg the register address
N * @param  pData the stored data pointer
N * @param  Length the length
N * @retval 0 in case of success, an error code otherwise
N */
Nstatic int32_t ReadAccRegWrap(void *Handle, uint8_t Reg, uint8_t *pData, uint16_t Length)
N{
N  LSM303AGR_ACC_Object_t *pObj = (LSM303AGR_ACC_Object_t *)Handle;
N
N  if (pObj->IO.BusType == LSM303AGR_I2C_BUS) /* I2C */
X  if (pObj->IO.BusType == 0U)  
N  {
N    /* Enable Multi-byte read */
N    return pObj->IO.ReadReg(pObj->IO.Address, (Reg | 0x80U), pData, Length);
N  }
N  else   /* SPI 3-Wires */
N  {
N    /* Enable Multi-byte read */
N    return pObj->IO.ReadReg(pObj->IO.Address, (Reg | 0x40U), pData, Length);
N  }
N}
