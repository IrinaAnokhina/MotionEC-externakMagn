N/**
N * @brief  Get the LSM303AGR accelerometer sensor raw axes
N * @param  pObj the device pObj
N * @param  Value pointer where the raw values of the axes are written
N * @retval 0 in case of success, an error code otherwise
N */
Nint32_t LSM303AGR_ACC_GetAxesRaw(LSM303AGR_ACC_Object_t *pObj, LSM303AGR_AxesRaw_t *Value)
N{
N  int16_t divisor = 1;
N  axis3bit16_t data_raw;
N  int32_t ret = LSM303AGR_OK;
X  int32_t ret = 0;
N  lsm303agr_op_md_a_t op_mode;
N
N  /* Read operative mode from sensor. */
N  if (lsm303agr_xl_operating_mode_get(&(pObj->Ctx), &op_mode) != LSM303AGR_OK)
X  if (lsm303agr_xl_operating_mode_get(&(pObj->Ctx), &op_mode) != 0)
N  {
N    return LSM303AGR_ERROR;
X    return -1;
N  }
N
N  /* Store the sensitivity based on actual full scale. */
N  switch (op_mode)
N  {
N    case LSM303AGR_HR_12bit:
N      divisor = 16;
N      break;
N
N    case LSM303AGR_NM_10bit:
N      divisor = 64;
N      break;
N
N    case LSM303AGR_LP_8bit:
N      divisor = 256;
N      break;
N
N    default:
N      ret = LSM303AGR_ERROR;
X      ret = -1;
N      break;
N  }
N
N  if (ret == LSM303AGR_ERROR)
X  if (ret == -1)
N  {
N    return ret;
N  }
N
N  /* Read raw data values. */
N  if (lsm303agr_acceleration_raw_get(&(pObj->Ctx), data_raw.u8bit) != LSM303AGR_OK)
X  if (lsm303agr_acceleration_raw_get(&(pObj->Ctx), data_raw.u8bit) != 0)
N  {
N    return LSM303AGR_ERROR;
X    return -1;
N  }
N
N  /* Format the data. */
N  Value->x = (data_raw.i16bit[0] / divisor);
N  Value->y = (data_raw.i16bit[1] / divisor);
N  Value->z = (data_raw.i16bit[2] / divisor);
N
N  return ret;
N}
