N                           (uint8_t*)&(val->a_wrist_tilt_mask), 1);
N  }
N  if(ret == 0){
N    ret = lsm6dsl_mem_bank_set(ctx, LSM6DSL_USER_BANK);
N  }
N
N  return ret;
N}
