N  ret = lsm6dsl_read_reg(ctx, LSM6DSL_WAKE_UP_SRC,
X  ret = lsm6dsl_read_reg(ctx, 0x1BU,
N                         (uint8_t*)&(val->wake_up_src), 1);
N  if(ret == 0){
N    ret = lsm6dsl_read_reg(ctx, LSM6DSL_TAP_SRC,
